	</select>
	
	<!-- 查询人员基本信息 -->
	<!-- 常控预警详情-->
    <select id="queryJbxx" parameterClass="ckyj" resultClass="ckyj">
        select *  from syrk_syrkxxzb a  
		left join (select * from ZDRY_ZDRYZB_NEW c where c.xt_zhxgsj = (select max(d.xt_zhxgsj) from ZDRY_ZDRYZB_NEW d)) e on e.zjhm = a.zjhm
