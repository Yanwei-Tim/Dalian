	<select id="cyryPagination" parameterClass="Map" resultClass="cyryxxb">
	<![CDATA[ select * from ( select rownum r ,m.* from (
				select * from DW_CYRYXXB t where t.dwid =#dwid# and t.xt_zxbz = '0' 
				 order by $sort$ $order$) m  
