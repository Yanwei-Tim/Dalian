	<!-- 保安员开始 -->
	<select id="bay_query_all" parameterClass="Map" resultClass="dwxyaqxxbBay">
      <![CDATA[
			SELECT * FROM ( SELECT ROWNUM r ,m.* FROM (	SELECT A.* FROM DW_DWBAY A
