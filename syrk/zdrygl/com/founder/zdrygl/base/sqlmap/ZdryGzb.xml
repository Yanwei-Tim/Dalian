<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ZdryGzb">
	
	<typeAlias alias="zdryGzb" type="com.founder.zdrygl.base.model.ZdryGzb" />
		
	<!--根据重点人员类型查询单条-->
	<select id="queryLglxByZdrylx" parameterClass="Map" resultClass="zdryGzb">
		<![CDATA[ 
			select * from ZDRY_GZB t 
			where zdrylx = #zdrylx# 
			and QYDM = #qydm#
			and XT_ZXBZ = '0' and rownum = 1  
		]]>
	</select>
	
</sqlMap>